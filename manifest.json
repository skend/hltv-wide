{
    "manifest_version": 2,

    "name": "hltv-wide",
    "version": "1.5",
    "description": "Makes hltv.org wider.",
    "permissions": ["tabs", "http://*/*"],
    "icons": { "128": "icon.png" },
    "content_scripts" : [{
        "matches" : ["https://www.hltv.org/*"],
        "js" : ["jquery.js", "fold-comments.js"],
        "css" : ["styles.css"]
    }]
}